<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="All Test Suite">
    <parameter name="browser" value=""/>
    <test name="login_with_invalid_data">
        <classes>
            <class name="testcases.T01_login">
                <methods>
                    <include name="login_with_empty_password"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="login_with_invalid_data2">
        <classes>
            <class name="testcases.T01_login">
                <methods>
                    <include name="login_with_empty_username"/>
                </methods>
            </class>
        </classes>
    </test>



    <test name="login_with_invalid_data3">
        <classes>
            <class name="testcases.T01_login">
                <methods>
                    <include name="login_with_empty_username_and_password"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="login_with_invalid_data4">
        <classes>
            <class name="testcases.T01_login">
                <methods>
                    <include name="login_with_valid_username_and_invalid_password"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="login_with_invalid_data5">
        <classes>
            <class name="testcases.T01_login">
                <methods>
                    <include name="login_with_invalid_username_and_valid_password"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="login_with_invalid_data6">
        <classes>
            <class name="testcases.T01_login">
                <methods>
                    <include name="login_with_invalid_username_and_invalid_password"/>
                </methods>
            </class>
        </classes>
    </test>
